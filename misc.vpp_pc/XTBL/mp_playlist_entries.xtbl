	<TableDescription>
		<Name>playlist_entries</Name>
		<Type>TableDescription</Type>
		<Element>
			<Name>Name</Name>
			<Type>String</Type>
			<Description>This is a mandatory field required for the table file to exist, but will be ignored by the game.</Description>
		</Element>
		<Element>
			<Name>mode</Name>
			<Type>Reference</Type>
			<Reference>
				<File>mp_modes.xtbl</File>
